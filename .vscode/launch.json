{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "TS File",
      "type": "node",
      "request": "launch",
      "args": ["${file}"],
      "runtimeArgs": [
        "-r",
        "ts-node/register"
      ],
      "cwd": "${workspaceRoot}",
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}

{
  "name": "@saulonunesdev/boootstrap",
  "version": "0.0.1",
  "description": "A TS Bootstrap BoilerPlate",
  "author": "Saulo Nunes <saulo@taverna.dev> (https://saulo.taverna.dev)",
  "license": "MIT",
  "scripts": {
    "prepare": "husky",
    "precommit": "lint-staged",
    "test": "npm-run-all -s test:*",
    "test:depcheck": "depcheck .",
    "test:lockfile": "lockfile-lint -p package-lock.json -t npm -a registry.npmjs.org -s",
    "test:package": "npmPkgJsonLint .",
    "test:audit": "npm audit --omit=dev",
    "test:format": "prettier . --check",
    "test:eslint": "eslint .",
    "test:ts": "tsc -p tsconfig.build.json --noemit",
    "test:jest": "jest --silent --no-cache",
    "fix": "npm-run-all -s fix:*",
    "fix:format": "prettier . --write",
    "fix:eslint": "eslint . --fix",
    "start": "node build/index.js",
    "compile": "tsc -p tsconfig.build.json"
  },
  "keywords": [
    "boilerplate",
    "bootstrap",
    "eslint-config-love",
    "git",
    "hooks",
    "husky",
    "linters",
    "repo",
    "standard",
    "typescript"
  ],
  "devDependencies": {
    "@jest/globals": "^30.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "depcheck": "^1.4.7",
    "eslint": "^9.34.0",
    "eslint-config-love": "^121.0.0",
    "eslint-plugin-jsonc": "^2.20.1",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "lint-staged": "^16.1.5",
    "lockfile-lint": "^4.12.1",
    "npm-package-json-lint": "^9.0.0",
    "npm-package-json-lint-config-default": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
